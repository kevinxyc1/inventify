<h2>Search Inventory</h2>
<form action='/items' method="GET">
    <div>
        <label>Name</label>
        <input type="text" name="name" value="<%= searchOptions.name%>">
    </div>
    <div>
        <label>Added After</label>
        <input type="date" name="dateAddedAfter" value="<%=searchOptions.dateAddedAfter%>">
    </div>
    <div>
        <label>Added Before</label>
        <input type="date" name="dateAddedBefore" value="<%=searchOptions.dateAddedBefore%>">
    </div>
    <button type="submit">Search</button>
</form>
<div>
    <% items.forEach(item => { %>
        <img height="150" width="100" src="<%= item.coverImagePath %>">
    <% }) %>
</div>